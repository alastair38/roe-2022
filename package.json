{
  "name": "blockhaus-starter",
  "version": "1.0.0",
  "description": "Starter hybrid theme with bundled block patterns and easy to transform to FSE.",
  "author": "Alastair Cox",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://github.com/Automattic/_s#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/_s.git"
  },
  "bugs": {
    "url": "https://github.com/Automattic/_s/issues"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.2",
    "@tailwindcss/typography": "^0.5.2",
    "@wordpress/scripts": "^19.2.2",
    "autoprefixer": "^10.4.7",
    "dir-archiver": "^1.1.1",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.13",
    "postcss-import": "^14.1.0",
    "rtlcss": "^3.5.0",
    "tailwindcss": "^3.0.24"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "dev": "tailwindcss -i ./styles/tailwind.css -o ./styles/style.css --postcss",
    "watch": "tailwindcss -i ./styles/tailwind.css -o ./styles/style.css --postcss --watch",
    "production": "tailwindcss -i ./styles/tailwind.css -o ./styles/style.css --postcss --minify",
    "test:chrome": "npx playwright test --headed --browser=chromium",
    "test:firefox": "npx playwright test --headed --browser=firefox",
    "test:safari": "npx playwright test --headed --browser=webkit"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.2"
  }
}
